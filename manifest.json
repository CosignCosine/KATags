{
  "manifest_version": 2,
  "name": "Scott's KA Tools",
  "description": "Scott's tools for KA. 0.2.0: Added basis for KATags",
  "version": "0.2.0",
  "content_scripts": [
    {
      "js": ["kablock.js", "katags.js"],
      "matches": [
        "*://*.khanacademy.org/*"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "earth.png",
    "default_popup": "popup.html"
  }
}
