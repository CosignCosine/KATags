{
  "manifest_version": 2,
  "name": "Scott's KA Tools",
  "description": "A large extension.",
  "version": "0.0.4",
  "content_scripts": [
    {
      "matches": [
        "*://*.khanacademy.org/*"
      ],
      "js": ["inject.js"]
    }
  ],
  "browser_action": {
    "default_icon": "earth.png",
    "default_popup": "popup.html"
  }
}
